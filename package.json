{
    "type": "commonjs",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "upgrade": "ncu --reject sass --upgrade && npm install && npm audit fix",
        "audit": "npm audit fix",
        "postinstall": "node ./postinstall.js",
        "dev": "npgsqlrest ./backend/cfg/server.json -o ./backend/cfg/build-id-header.json ./backend/cfg/dev.json -o ./backend/cfg/local.json",
        "start": "npgsqlrest ./backend/cfg/server.json -o ./backend/cfg/build-id-header.json ./backend/cfg/dev.json -o ./backend/cfg/local.json",
        "server": "npgsqlrest ./backend/cfg/server.json -o ./backend/cfg/build-id-header.json",
        "version": "npgsqlrest -v",
        "build": "node ./frontend/cfg/build.js",
        "build-index": "node ./frontend/cfg/build.js index",
        "build-admin": "node ./frontend/cfg/build.js admin",
        "watch": "node ./frontend/cfg/build.js --watch",
        "watch-index": "node ./frontend/cfg/build.js index --watch",
        "watch-admin": "node ./frontend/cfg/build.js admin --watch",
        "scss": "node ./frontend/cfg/build.js --style",
        "scss-watch": "node ./frontend/cfg/build.js --style --watch",
        "data": "azuredatastudio ./backend/src",
        "up": "pgmigrations up --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "up-list": "pgmigrations up --list --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "up-dump": "pgmigrations up --dump --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "history": "pgmigrations history --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "test": "pgmigrations test --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "test-list": "pgmigrations test --list --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "schema": "pgmigrations schema --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "psql": "pgmigrations psql --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js"
    },
    "dependencies": {
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-commonjs": "^28.0.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-replace": "^6.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.0",
        "@tsconfig/svelte": "^5.0.4",
        "@types/bootstrap": "^5.2.10",
        "bootstrap": "5.3.3",
        "bootstrap-icons": "^1.11.3",
        "rollup": "^4.22.5",
        "rollup-plugin-css-only": "^4.5.2",
        "rollup-plugin-svelte": "^7.2.2",
        "sass": "1.77.6",
        "svelte": "^4.2.19",
        "svelte-check": "^4.0.4",
        "svelte-preprocess": "^6.0.3",
        "tslib": "^2.7.0",
        "typescript": "^5.6.2"
    },
    "devDependencies": {
        "npm-check-updates": "^17.1.3",
        "npgsqlrest": "^2.1.0",
        "@vbilopav/pgmigrations": "^0.9.2"
    }
}
