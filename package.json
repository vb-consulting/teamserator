{
    "type": "commonjs",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "upgrade": "ncu --upgrade && npm install && npm audit fix",
        "audit": "npm audit fix",
        "postinstall": "node ./postinstall.js",
        "start": "npgsqlrest ./backend/cfg/appsettings.json ./backend/cfg/appsettings-server.json -o ./backend/cfg/appsettings-build-id-header.json ./backend/cfg/appsettings-dev.json -o ./backend/cfg/appsettings-local.json",
        "build": "node ./frontend/cfg/build.js",
        "build-index": "node ./frontend/cfg/build.js index",
        "build-admin": "node ./frontend/cfg/build.js admin",
        "watch": "node ./frontend/cfg/build.js --watch",
        "watch-index": "node ./frontend/cfg/build.js index --watch",
        "watch-admin": "node ./frontend/cfg/build.js admin --watch",
        "scss": "node ./frontend/cfg/build.js --style",
        "scss-watch": "node ./frontend/cfg/build.js --style --watch",
        "data": "azuredatastudio ./backend/src",
        "db-up": "pgmigrations up --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "db-up-list": "pgmigrations up --list --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "db-up-dump": "pgmigrations up --dump --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "db-history": "pgmigrations history --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "db-test": "pgmigrations test --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "db-test-list": "pgmigrations test --list --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "db-schema": "pgmigrations schema --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js",
        "psql": "pgmigrations psql --config=./backend/cfg/pgmigrations.js --config=./backend/cfg/pgmigrations-local.js"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.7",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@tsconfig/svelte": "^5.0.4",
        "@types/bootstrap": "^5.2.10",
        "bootstrap": "5.3.3",
        "bootstrap-icons": "^1.11.3",
        "npgsqlrest": "^1.2.6",
        "npm-check-updates": "^16.14.20",
        "rollup": "^4.18.0",
        "rollup-plugin-css-only": "^4.5.2",
        "rollup-plugin-svelte": "^7.2.2",
        "sass": "^1.77.5",
        "svelte": "^4.2.18",
        "svelte-check": "^3.8.0",
        "svelte-preprocess": "^5.1.4",
        "tslib": "^2.6.3",
        "typescript": "^5.4.5",
        "@vbilopav/pgmigrations": "^0.3.1"
    }
}
